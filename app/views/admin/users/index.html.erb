<div class="page-header">
  <h1>Users</h1>
  <ul class="actions">
    <li>
      <%= link_to "New User", new_admin_user_path(), class: "new-btn" %>
    </li>
  </ul>
</div>

<div class="container">
  <div class="row header-row">
    <div class="col-xs-6">
      Email
    </div>
    <div class="col-xs-3">
      User Created 
    </div>
    <div class="col-xs-3">
      Last Logged In
    </div>
  </div>
  <% @users.each do |u| %>
    <div class="row">
      <div class="col-xs-6">
       <%= link_to u, [:admin, u] %>
      </div>
      <div class="col-xs-3">
        <%= u.create_date %> 
      </div>
      <div class="col-xs-3">
        <%=  u.last_log_in %> 
      </div>
    </div>
  <% end %>
</div>